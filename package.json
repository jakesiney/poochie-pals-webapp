{
  "name": "poochie-pals",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext",
    "prettier-format": "prettier --config .prettierrc './**' --write",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "sass --no-source-map lib/_sass/main.scss:public/stylesheets/main.css",
    "css-postcss": "postcss --use autoprefixer --output public/stylesheets/main.css public/stylesheets/main.css",
    "css-watch": "npm run css-build -- --watch",
    "deploy": "npm run css-deploy && npm run js-build",
    "js-build": "babel lib/_javascript --out-dir public/javascript",
    "js-watch": "npm run js-build -- --watch",
    "serve": "node ./bin/www",
    "start": "npm-run-all --parallel css-watch js-watch serve",
    "db:seed": "node ./bin/seed-db.js",
    "test": "jest"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bulma": "^0.9.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.11.2",
    "s3-streamlogger": "^1.9.0",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.9",
    "autoprefixer": "^10.4.7",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "bulma": "^0.9.3",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^3.0.0",
    "sass": "^1.51.0",
    "supertest": "^6.3.3"
  }
}
